<resources>
    <!-- Base application theme. -->
    <style name="Theme.UnsplashExample" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/md_red_A700</item>
        <item name="colorPrimaryVariant">@color/black</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/darknewbgsec</item>

        <!-- Customize your theme here. -->
    </style>


    <style name="SCBSwitch" parent="Theme.AppCompat.Light">
        <!-- active thumb & track color (30% transparency) -->
        <item name="colorControlActivated">@color/teal_200</item>

        <!-- inactive thumb color -->
        <item name="colorSwitchThumbNormal">@color/md_red_300</item>

        <!-- inactive track color (30% transparency) -->
        <item name="android:colorForeground">#6DFFFFFF</item>
    </style>


</resources>